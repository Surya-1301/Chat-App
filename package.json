{
  "name": "chat-app-monorepo",
  "private": true,
  "scripts": {
  "start:backend": "cd backend && npm run dev",
  "start:frontend": "cd frontend && npm start",
  "dev": "npm-run-all --parallel start:backend start:frontend",
  "start": "npm-run-all --parallel start:backend start:frontend"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "expo": "^53.0.20",
    "mongodb": "^6.19.0"
  }
}
